{
  "name": "tibber-aws",
  "version": "5.1.1",
  "description": "",
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "ts-node -T samples/sample1.ts",
    "test": "ava  --verbose -s",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix && sortier \"./**/*.{ts,tsx,js,jsx}\"",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint",
    "release": "semantic-release",
    "release:local": "run.env semantic-release",
    "release:preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "cz": "git status && cz",
    "cz:all": "git add . && git status && cz",
    "cz:init": "commitizen init cz-conventional-changelog --yarn --dev --exact"
  },
  "files": [
    "/dist/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tibbercom/tibber-aws.git"
  },
  "homepage": "https://github.com/tibbercom/tibber-aws#readme",
  "author": "tibber",
  "license": "ISC",
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "dist/src/",
        "test/": "dist/test/"
      }
    }
  },
  "devDependencies": {
    "@ava/typescript": "^1.1.1",
    "@semantic-release/git": "^9.0.0",
    "@snowcoders/sortier": "^3.2.0",
    "@types/node": "^14.11.2",
    "@types/randomstring": "^1.1.6",
    "ava": "^3.8.2",
    "commitizen": "^4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "gts": "^3.0.3",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3",
    "randomstring": "^1.1.5",
    "run.env": "^1.1.0",
    "semantic-release": "^17.3.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.686.0",
    "sync-rpc": "^1.3.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
