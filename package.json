{
  "name": "tibber-aws",
  "version": "5.4.6",
  "description": "",
  "main": "./dist/src/index.js",
  "scripts": {
    "start": "ts-node -T samples/sample1.ts",
    "test": "jest  --verbose",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix && sortier \"./**/*.{ts,tsx,js,jsx}\"",
    "prepare": "yarn run compile",
    "pretest": "yarn run compile",
    "posttest": "yarn run lint",
    "release": "semantic-release",
    "release:local": "dotenv semantic-release",
    "release:preview": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
    "cz": "git status && cz",
    "cz:all": "git add . && git status && cz",
    "cz:init": "commitizen init cz-conventional-changelog --yarn --dev --exact"
  },
  "files": [
    "/dist/src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tibber/tibber-aws.git"
  },
  "homepage": "https://github.com/tibber/tibber-aws#readme",
  "author": "tibber",
  "license": "ISC",
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^10.0.0",
    "@snowcoders/sortier": "^3.2.0",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.4",
    "@types/randomstring": "^1.1.7",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^4.0.0",
    "gts": "^3.1.0",
    "husky": "^7.0.2",
    "jest": "^27.2.1",
    "lint-staged": "^10.5.3",
    "randomstring": "^1.2.1",
    "semantic-release": "^19.0.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.0.3"
  },
  "engines": {
    "node": ">=9.0.0"
  },
  "dependencies": {
    "aws-sdk": "^2.1154.0",
    "sync-rpc": "^1.3.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
